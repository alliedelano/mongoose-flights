<body>
    <h1>Flight List</h1>
    <ul>
        <% flights.forEach(f => { %>
            <li>
               <%= f.airline %>  
               - 
               <%= f.airport %> 
               - 
               <%= f.flightNo %> 
               - 
               <%= f.departs %> 
            </li>
        <% }) %> 
    </ul>
    <div id="footer"></div>
</html>